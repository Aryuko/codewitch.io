{
    "extends": [
        "eslint:recommended",
        "next/core-web-vitals",
        "plugin:react/recommended",
        "plugin:prettier/recommended",
        "plugin:react-hooks/recommended"
    ],
    "rules": {
        "no-console": "warn",
        "no-unused-vars": "off",
        "react/prop-types": "off",
        "react/no-unescaped-entities": "off",
        "prettier/prettier": "warn"
    }
}
